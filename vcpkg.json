{
  "name": "voxer",
  "version-semver": "0.2.1",
  "maintainers": [
    "Denys Asauliak <denoming@gmail.com>"
  ],
  "supports": "linux and !windows",
  "dependencies": [
    "fmt",
    "spdlog",
    "nlohmann-json",
    "piper-phonemize",
    "utfcpp"
  ],
  "features": {
    "tests": {
      "description": "Testing support",
      "dependencies": [
        "gtest"
      ]
    },
    "formatted": {
      "description": "Formatted audio data support",
      "dependencies": [
        {
          "name": "libsndfile",
          "default-features": false
        }
      ]
    }
  }
}
