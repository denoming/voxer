{
  "name": "voxer",
  "version-semver": "0.2.6",
  "maintainers": [
    "Denys Asauliak <denoming@gmail.com>"
  ],
  "supports": "linux and !windows",
  "dependencies": [
    "fmt",
    "spdlog",
    "nlohmann-json",
    "utfcpp"
  ],
  "features": {
    "cli": {
      "description": "Enable CLI feature",
      "dependencies": [
        "cxxopts"
      ]
    },
    "tests": {
      "description": "Enable unit tests",
      "dependencies": [
        "gtest"
      ]
    },
    "formatted": {
      "description": "Enable formatted handlers feature",
      "dependencies": [
        {
          "name": "libsndfile",
          "default-features": false
        }
      ]
    }
  }
}
